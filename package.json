    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "image-size": "^1.0.2",
    "next": "13.5.6",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "pliny": "^0.1.5",
